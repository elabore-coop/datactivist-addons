<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <template id="portal_my_home" name="Show Deliverables" customize_show="True"
        inherit_id="portal.portal_my_home" priority="40">
        <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
            <t t-call="portal.portal_docs_entry">
                <t t-set="title">Deliverables</t>
                <t t-set="url" t-value="'/my/deliverables'" />
                <t t-set="placeholder_count" t-value="'deliverables_count'" />
            </t>
        </xpath>
    </template>
</odoo>